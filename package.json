{
  "name": "personal-nest-boilerplate",
  "version": "0.0.1",
  "description": "Personal NestJS Boilerplate, Typescript, Postgres, TypeORM",
  "author": "Omid Astaraki <omid.astaraki@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f ormconfig -d src/migrations -n",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig",
    "watch:dev": "nodemon --config nodemon.json",
    "debug:dev": "nodemon --config nodemon-debug.json",
    "webpack": "webpack --config webpack.config.js --progress",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f ormconfig -d src/migrations -n",
    "schema:drop": "ts-node node_modules/typeorm/cli.js schema:drop -f ormconfig",
    "start:prod": "node dist/main.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docs:dev": "vuepress dev -p 7070",
    "docs:build": "DEPLOY_ENV=gh-pages vuepress build",
    "docs:deploy": "yarn docs:build && gh-pages -d .vuepress/dist"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/core": "^7.0.9",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/microservices": "^7.0.9",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.9",
    "@nestjs/swagger": "^4.5.5",
    "@nestjs/typeorm": "^7.0.0",
    "aws-sdk": "~2.673.0",
    "bcrypt": "~4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compression": "~1.7.4",
    "cross-env": "^7.0.2",
    "dotenv": "~8.2.0",
    "express": "~4.17.1",
    "express-rate-limit": "~5.1.3",
    "file-type": "~14.3.0",
    "ghasedak": "^0.0.2",
    "helmet": "~3.22.0",
    "jsonwebtoken": "~8.5.1",
    "lodash": "^4.17.15",
    "mime-types": "~2.1.27",
    "morgan": "~1.10.0",
    "neverthrow": "^2.2.1",
    "passport": "~0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.1.0",
    "reflect-metadata": "~0.1.13",
    "request-context": "~2.0.0",
    "rxjs": "^6.5.3",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.25",
    "typeorm-transactional-cls-hooked": "^0.1.8",
    "typescript": "^3.8.3",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/testing": "^7.0.9",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/express-rate-limit": "^5.0.0",
    "@types/file-type": "^10.9.1",
    "@types/helmet": "^0.0.46",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lodash": "^4.14.150",
    "@types/mime-types": "^2.1.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.13.5",
    "@types/passport-jwt": "^3.0.2",
    "@types/supertest": "^2.0.9",
    "@types/uuid": "^7.0.3",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "gh-pages": "^2.1.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "~10.2.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.3",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "~1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "vuepress": "^1.4.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  }
}
